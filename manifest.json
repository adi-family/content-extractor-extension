{
  "manifest_version": 3,
  "name": "Content Extractor",
  "version": "1.0",
  "description": "AI-powered content extraction and highlighting by ADI Family",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["content.css"]
    }
  ]
}
